{
  "manifest_version": 3,
  "name": "AMZ SQP Fetcher",
  "version": "1.0",
  "description": "Fetches data from Search Query Perfomance and downloads it as a CSV.",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "host_permissions": ["<all_urls>"],


  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
}